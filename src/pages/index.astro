	<script type="text/javascript">
		const carousel = document.querySelector('.carousel');
		let isDragging = false;
		let startX;
		let scrollLeft;
		let autoScrollInterval;
		let userInteractionTimeout;

		// Función para mover el carrusel automáticamente
		function startAutoScroll() {
			autoScrollInterval = setInterval(() => {
			carousel.scrollBy({
				left: carousel.offsetWidth / 3, // Ajusta el valor para desplazar una tarjeta
				behavior: 'smooth'
			});
			}, 5000); // Cambia cada 3 segundos
		}

		// Pausar el desplazamiento automático
		function stopAutoScroll() {
			clearInterval(autoScrollInterval);
		}

		// Reiniciar el desplazamiento automático después de una pausa
		function restartAutoScroll() {
			clearTimeout(userInteractionTimeout);
			userInteractionTimeout = setTimeout(() => {
			startAutoScroll();
			}, 5000); // Espera 5 segundos después de la última interacción del usuario
		}

		// Funciones para manejar el drag del carrusel
		carousel.addEventListener('mousedown', (e) => {
			isDragging = true;
			carousel.classList.add('dragging');
			startX = e.pageX - carousel.offsetLeft;
			scrollLeft = carousel.scrollLeft;
			document.body.style.userSelect = 'none';
			carousel.style.cursor = 'grabbing';

			stopAutoScroll(); // Detener el desplazamiento automático al arrastrar
		});

		carousel.addEventListener('mouseleave', () => {
			isDragging = false;
			carousel.classList.remove('dragging');
			carousel.style.cursor = 'grab';
			document.body.style.userSelect = '';
			restartAutoScroll(); // Reiniciar el desplazamiento automático al dejar de interactuar
		});

		carousel.addEventListener('mouseup', () => {
			isDragging = false;
			carousel.classList.remove('dragging');
			carousel.style.cursor = 'grab';
			document.body.style.userSelect = '';
			restartAutoScroll(); // Reiniciar el desplazamiento automático al dejar de interactuar
		});

		carousel.addEventListener('mousemove', (e) => {
			if (!isDragging) return;
			e.preventDefault();
			const x = e.pageX - carousel.offsetLeft;
			const walk = (x - startX) * 2; // Velocidad de desplazamiento
			carousel.scrollLeft = scrollLeft - walk;
		});

		// Iniciar el desplazamiento automático cuando la página carga
		startAutoScroll();
	</script>
